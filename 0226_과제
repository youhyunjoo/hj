import random

ans1="제 대답은요......숙제가 참 쉽죠"
ans2="제 대답은요......누가 챗봇소리를 내었는가"

print("반갑습니다. 최첨단 인공지능 대화로봇을 지향만합니다")


while True:
    x = input('아무거나 물어보세요~\n>')
    if x == 'Quit':
        break
    elif x=='':
        print("에이~ 그러지 말고 아무말이나 해봐요")
    else :
        choice=random.randint(1,2)
        if choice==1:
            answer=ans1
        elif choice==2:
            answer=ans2
        print(answer)

input("엔터치면 마칩니다")
